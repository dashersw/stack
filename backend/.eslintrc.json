{
  "env": {
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "extends": [
    "airbnb",
    "prettier",
    "prettier/standard"
  ],
  "rules": {
    "eqeqeq": "off",
    "class-methods-use-this": "off",
    "eslint-plugin-import/no-extraneous-dependencies": "off"
  },
  "settings": {
    "react": {
      "version": "latest"
    }
  }
}
